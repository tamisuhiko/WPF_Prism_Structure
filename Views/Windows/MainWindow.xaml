<syncfusion:ChromelessWindow x:Class="WPFStructure.Views.MainWindow"
                             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                             xmlns:prism="http://prismlibrary.com/"
                             xmlns:Commons="clr-namespace:WPFStructure.Commons"
                             prism:ViewModelLocator.AutoWireViewModel="True"
                             Title="{Binding Title}"
                             Height="550"
                             Width="1024"
                             xmlns:syncfusion="http://schemas.syncfusion.com/wpf"
                             xmlns:uc="clr-namespace:WPFStructure.Views"
                             xmlns:syncfusionskin="clr-namespace:Syncfusion.SfSkinManager;assembly=Syncfusion.SfSkinManager.WPF"
                             syncfusionskin:SfSkinManager.Theme="{syncfusionskin:SkinManagerExtension ThemeName=Windows11Dark}">
    <uc:ContainerView />

</syncfusion:ChromelessWindow>
